// deletion of elements
#include<stdio.h>
int main(){
	int arr[]={1,2,3,4,5};
	int i,len,ind;
	len=sizeof(arr)/sizeof(int);
	scanf("%d",&ind);
	if(ind>=0 && ind<len){
	for(i=ind;i<len-1;i++)
		arr[i]=arr[i+1];
		len--;
	for(i=0;i<len;i++){
		printf("%d",arr[i]);
	}}
	else
	printf("Wrong input!");
	
}
